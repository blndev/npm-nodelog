{
  "name": "nodelog",
  "version": "0.3.1",
  "description": "simple node logging utility with colored output and log level support",
  "main": "index.js",
  "scripts": {
    "start": "node dev/run.js",
    "test": "node ./node_modules/mocha/bin/_mocha",
    "istanbul": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000",
    "stylecheck": "./node_modules/jshint/bin/jshint --reporter=node_modules/jshint-stylish index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blndev/npm-nodelog.git"
  },
  "keywords": [
    "node",
    "log",
    "color",
    "level",
    "loglevel",
    "output",
    "console",
    "console.log"
  ],
  "author": "blndev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blndev/npm-nodelog/issues"
  },
  "files": [
    "LICENSE",
    "Readme.md",
    "index.js"
  ],
  "homepage": "https://github.com/blndev/npm-nodelog",
  "dependencies": {
    "cli-color": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "gulp": "^3.9.1",
    "gulp-jshint": "^2.0.1",
    "istanbul": "^0.4.3",
    "jshint": "^2.9.2",
    "jshint-stylish": "^2.2.0",
    "mocha": "^2.5.3"
  }
}
